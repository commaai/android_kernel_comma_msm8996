/ {
	model = "Letv Technologies, Inc. MSM 8996pro + PMI8996 LE_ZL1";
	compatible = "qcom,msm8996-mtp", "qcom,msm8996", "qcom,mtp";
  // X727 is <8 0xA2>
  // X722 is also <8 0xA2> ...
	qcom,board-id = <8 0xA1>, <8 0xA2>, <8 0xA4>, <8 0xA5>;
	comma,board-name = "leeco";

	aliases {
		i2c9 = &i2c_9;
	};

	reserved-memory {
		ramoops_mem: ramoops_region@91500000 {
			compatible = "shared-dma-pool";
			no-map;
			reg = <0 0x91500000 0 0x00100000>;
			label = "ramoops_mem";
		};
	};
};

&tlmm {
	/delete-node/ cam_sensor_front_active;
	/delete-node/ cam_sensor_front_suspend;
	/delete-node/ cam_sensor_rear_active;
	/delete-node/ cam_sensor_rear_suspend;

	cam_sensor_front_active: cam_sensor_front_active {
		mux {
			pins = "gpio23";
			function = "gpio";
		};

		config {
			pins = "gpio23";
			bias-disable;
			drive-strength = <2>;
		};
	};

	cam_sensor_front_suspend: cam_sensor_front_suspend {
		mux {
			pins = "gpio23";
			function = "gpio";
		};

		config {
			pins = "gpio23";
			bias-disable;
			drive-strength = <2>;
		};
	};

	cam_sensor_rear_active: cam_sensor_rear_active {
		mux {
			pins = "gpio29";
			function = "gpio";
		};

		config {
			pins = "gpio29";
			bias-disable;
			drive-strength = <2>;
		};
	};

	cam_sensor_rear_suspend: cam_sensor_rear_suspend {
		mux {
			pins = "gpio29";
			function = "gpio";
		};

		config {
			pins = "gpio29";
			bias-disable;
			drive-strength = <2>;
		};
	};

	i2c_9 {
		i2c_9_active: i2c_9_active {
			mux {
				pins = "gpio51", "gpio52";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio51", "gpio52";
				drive-strength = <2>;
				bias-disable;
			};
		};

		i2c_9_sleep: i2c_9_sleep {
			mux {
				pins = "gpio51", "gpio52";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio51", "gpio52";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	synaptics_irq {
		synaptics_irq_active: synaptics_irq_active {
			mux {
				pins = "gpio125";
				function = "gpio";
			};

			config {
				pins = "gpio125";
				bias-pull-up;
				drive-strength = <16>;
			};
		};

		synaptics_irq_suspend: synaptics_irq_suspend {
			mux {
				pins = "gpio125";
				function = "gpio";
			};

			config {
				pins = "gpio125";
				bias-disable;
				drive-strength = <2>;
			};
		};
	};

	synaptics_reset {
		synaptics_reset_active: synaptics_reset_active {
			mux {
				pins = "gpio89";
				function = "gpio";
			};

			config {
				pins = "gpio89";
				bias-pull-up;
				drive-strength = <16>;
				output-low;
			};
		};

		synaptics_reset_suspend: synaptics_reset_suspend {
			mux {
				pins = "gpio89";
				function = "gpio";
			};

			config {
				pins = "gpio89";
				bias-disable;
				drive-strength = <2>;
				output-high;
			};
		};

  /****************focaltech reset + irq*********/
    focaltech_irq{
      focaltech_irq_active: focaltech_irq_active{
        mux {
          pins = "gpio125";
          function = "gpio";
        };  

        config {
          pins = "gpio125";
          bias-pull-up; /* PULL UP */
          drive-strength = <16>; /* 16 MA */
        };  
      };  

      focaltech_irq_suspend: focaltech_irq_suspend{
        mux {
          pins = "gpio125";
          function = "gpio";
        };  

        config {
          pins = "gpio125";
          bias-disable; /* No PULL */
          drive-strength = <2>; /* 2 MA */
        };  
      };  
    };  

    focaltech_reset{
      focaltech_reset_active: focaltech_reset_active{
        mux {
          pins = "gpio89";
          function = "gpio";
        };  
        config {
          pins = "gpio89";
          bias-pull-up;
          drive-strength = <16>; /* 16 MA */
          output-high;
        };  
      };  
      focaltech_reset_suspend: focaltech_reset_suspend{
        mux {
          pins = "gpio89";
          function = "gpio";
        };  
        config {
          pins = "gpio89";
          bias-disable; /* NO PULL  */
          drive-strength = <2>; /* 2 MA */
        };  
      };  
    };  
  /****************focaltech reset + irq*********/

	};
};

